{
  "name": "graphql-mock",
  "version": "1.1.0",
  "description": "GraphQL endpoint mockery library for testing",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "eslint '{src,test}/**/*.{ts,tsx}'",
    "prepare": "npm build",
    "test": "mocha"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MadRabbit/graphql-mock.git"
  },
  "author": "Nikolay Nemshilov",
  "license": "MIT",
  "dependencies": {
    "apollo-link-schema": "^1.2.2",
    "fast-deep-equal": "^2.0.1",
    "graphql-tools": "^4.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/graphql": "^14.2.0",
    "@types/jsdom": "^12.2.3",
    "@types/mocha": "^5.2.6",
    "@types/nock": "^10.0.3",
    "@types/node": "^12.0.2",
    "@types/node-fetch": "^2.3.4",
    "@types/react": "^16.8.17",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "eslint-config-compono-ui-ts": "^1.0.0",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "husky": "^2.3.0",
    "jsdom": "^15.1.0",
    "lint-staged": "^8.1.7",
    "mocha": "^6.1.4",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-apollo-hooks": "^0.4.5",
    "react-dom": "^16.8.6",
    "sinon": "^7.3.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  }
}
